{
  "name": "kvass-ordering-widget",
  "version": "0.1.0",
  "description": "Kvass Ordering Widget",
  "main": "./src/index.js",
  "scripts": {
    "start:dev": "NODE_ENV=development && ./node_modules/.bin/webpack --display-error-details && ./node_modules/.bin/http-server . -c-1",
    "start:dev:windows": "SET NODE_ENV=development && webpack --watch --display-error-details",
    "start:prod": "NODE_ENV=production && ./node_modules/.bin/webpack --display-error-details && ./node_modules/.bin/uglifyjs --compress -o dist/main.bundle.min.js -- dist/main.bundle.js && ./node_modules/.bin/http-server . -c-1",
    "autobuild:dev": "nodemon --exec npm run start:dev",
    "autobuild:prod": "nodemon --exec npm run start:prod",
    "lint": "eslint src/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shareactorIO/kvass-ordering-widget.git"
  },
  "author": "Sofien Rakke Lazreg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shareactorIO/kvass-ordering-widget/issues"
  },
  "homepage": "https://github.com/shareactorIO/kvass-ordering-widget#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "core-js": "^2.5.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "http-server": "^0.10.0",
    "node-sass": "^4.6.0",
    "nodemon": "^1.17.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "uglify-js": "^3.3.5"
  },
  "dependencies": {
    "@shareactor/shareactor-sdk": "0.3.1",
    "classnames": "2.2.5",
    "i18n-react": "0.6.4",
    "lodash.clonedeep": "4.5.0",
    "prop-types": "15.6.1",
    "react": "16.3.1",
    "react-custom-scrollbars": "4.2.1",
    "react-debounce-input": "3.2.0",
    "react-dom": "16.3.1",
    "react-notification-badge": "1.3.4",
    "reflux": "6.4.1"
  },
  "browser": "./dist/main.bundle.js"
}
